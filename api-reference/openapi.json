{
  "openapi": "3.0.1",
  "info": {
    "title": "OpenAPI Plant Store",
    "description": "A sample API that uses a plant store as an example to demonstrate features in the OpenAPI specification",
    "license": {
      "name": "MIT"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api-git-task-api-enrichment-improvements-internal-apps.vercel.app"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/company": {
      "get": {
        "description": "Returns people information",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "description": "ID of the company",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "abn",
            "in": "query",
            "description": "ABN of the company",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "fqdn",
            "in": "query",
            "description": "FQDN of the company",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "website",
            "in": "query",
            "description": "Website of the company",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ln_slug",
            "in": "query",
            "description": "LinkedIn slug of the company",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ln_url",
            "in": "query",
            "description": "LinkedIn URL of the company",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Company response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Company"
                }
              }
            }
          },
          "400": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/people": {
      "get": {
        "description": "Returns people information",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "description": "The id of the person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ln_slug",
            "in": "query",
            "description": "The linkedin slug of the person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ln_url",
            "in": "query",
            "description": "The linkedin url of the person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "work_email",
            "in": "query",
            "description": "The work email of the person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "personal_email",
            "in": "query",
            "description": "The personal email of the person",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Plant response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/People"
                }
              }
            }
          },
          "400": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Company": {
        "type": "object",
        "properties": {
          "org_registers": {
            "type": "object",
            "additionalProperties": true,
            "example": {
              "vc": "VCs & Investors"
            }
          },
          "linkedin_url": {
            "type": "string",
            "example": "https://www.linkedin.com/company/1234567890"
          },
          "name": {
            "type": "string",
            "example": "Company Name"
          },
          "locations": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Sydney", "Melbourne"]
          },
          "industries": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Finance", "Tech"]
          },
          "revenue": {
            "type": "string",
            "example": ">$1B+"
          },
          "keywords": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Finance", "Tech"]
          },
          "id": {
            "type": "string",
            "example": "f0000067890"
          },
          "website": {
            "type": "string",
            "example": "https://www.example.com"
          },
          "industry_codes": {
            "type": "object",
            "properties": {
              "abn": {
                "type": "string",
                "nullable": true,
                "example": "1234567890"
              },
              "acn": {
                "type": "string",
                "nullable": true,
                "example": "1234567890"
              },
              "sic": {
                "type": "string",
                "nullable": true,
                "example": "1234567890"
              },
              "duns": {
                "type": "string",
                "nullable": true,
                "example": "1234567890"
              },
              "naics": {
                "type": "string",
                "nullable": true,
                "example": "1234567890"
              },
              "anzsic": {
                "type": "string",
                "nullable": true,
                "example": "1234567890"
              }
            }
          },
          "social_media": {
            "type": "object",
            "properties": {
              "twitter": {
                "type": "string",
                "nullable": true,
                "example": "username"
              },
              "youtube": {
                "type": "string",
                "nullable": true,
                "example": "username"
              },
              "facebook": {
                "type": "string",
                "nullable": true,
                "example": "username"
              },
              "linkedin": {
                "type": "string",
                "nullable": true,
                "example": "username"
              },
              "instagram": {
                "type": "string",
                "nullable": true,
                "example": "username"
              }
            }
          },
          "au_employee_count": {
            "type": "integer",
            "nullable": true,
            "example": 1000
          },
          "hq_location": {
            "type": "string",
            "nullable": true,
            "example": "Wallan"
          },
          "abn_operation_status": {
            "type": "string",
            "enum": ["active", "inactive"],
            "example": "active"
          },
          "phones": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["0450 600 0000"]
          },
          "tagline": {
            "type": "string",
            "example": "We are a team of 1000 people"
          },
          "emails": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["email@example.com"]
          },
          "states": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["NSW", "VIC"]
          },
          "hq_country": {
            "type": "string",
            "example": "Australia"
          },
          "year_founded": {
            "type": "integer",
            "example": 2000
          },
          "company_size": {
            "type": "string",
            "example": "1000+"
          },
          "company_type": {
            "type": "string",
            "example": "Public company"
          },
          "description": {
            "type": "string",
            "example": "CommBank offers personal banking, business solutions, institutional banking, company information, and more"
          },
          "nextGen": {
            "type": "object",
            "properties": {
              "technographics": {
                "type": "object",
                "additionalProperties": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "example": {
                  "Software Development": ["PostgreSQL", "Java"],
                  "Finance & Accounting": ["Visa", "Pace"]
                }
              }
            }
          }
        }
      },
      "People": {
        "type": "object",
        "properties": {
          "linkedin": {
            "type": "string",
            "example": "https://www.linkedin.com/in/person_slug"
          },
          "name": {
            "type": "string",
            "example": "John Doe"
          },
          "is_firmable_verified_company": {
            "type": "boolean",
            "example": true
          },
          "month_joined": {
            "type": "number",
            "example": 2
          },
          "seniority": {
            "type": "string",
            "example": "C-Suite / Partner"
          },
          "headline": {
            "type": "string",
            "example": "Unlimited C-Suite Partner"
          },
          "keywords": {
            "type": "array",
            "items": {
              "type": "string",
              "nullable": true
            },
            "example": ["Finance", "Tech"]
          },
          "id": {
            "type": "string",
            "example": "fp000000067890"
          },
          "middle_name": {
            "type": "string",
            "nullable": true,
            "example": "M"
          },
          "department": {
            "type": "string",
            "example": "Marketing"
          },
          "location": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Sydney", "Melbourne"]
          },
          "social_media": {
            "type": "object",
            "properties": {
              "github": {
                "type": "string",
                "nullable": true,
                "example": "https://github.com/username"
              },
              "twitter": {
                "type": "object",
                "properties": {
                  "stars": {
                    "type": "integer",
                    "nullable": true,
                    "example": 100
                  },
                  "handle": {
                    "type": "string",
                    "nullable": true,
                    "example": "username"
                  },
                  "followers": {
                    "type": "integer",
                    "nullable": true,
                    "example": 100
                  },
                  "connections": {
                    "type": "integer",
                    "nullable": true,
                    "example": 100
                  }
                },
                "nullable": true
              }
            }
          },
          "current_company": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "example": "f0000067890"
              },
              "logo": {
                "type": "boolean",
                "example": true
              },
              "name": {
                "type": "string",
                "example": "Company Name"
              },
              "size": {
                "type": "string",
                "example": "11 - 50"
              },
              "website": {
                "type": "string",
                "example": "https://www.example.com"
              },
              "industry": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "example": ["Finance", "Tech"]
              },
              "linkedin": {
                "type": "string",
                "example": "person_slug"
              },
              "main_phones": {
                "type": "array",
                "nullable": true,
                "items": {
                  "type": "string"
                },
                "example": ["0450 600 0000"]
              },
              "other_phones": {
                "type": "array",
                "nullable": true,
                "items": {
                  "type": "string"
                },
                "example": ["0450 600 0000"]
              },
              "mobile_phones": {
                "type": "array",
                "nullable": true,
                "items": {
                  "type": "string"
                },
                "example": ["0450 600 0000"]
              }
            }
          },
          "time_in_current_role": {
            "type": "string",
            "example": "Jan '23 - Present"
          },
          "last_name": {
            "type": "string",
            "example": "Doe"
          },
          "phones": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "value": {
                  "type": "string",
                  "example": "0450 600 0000"
                },
                "is_dnd": {
                  "type": "boolean",
                  "example": false
                }
              }
            }
          },
          "emails": {
            "type": "object",
            "properties": {
              "personal": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "value": {
                      "type": "string",
                      "example": "email@firmable.com"
                    }
                  }
                }
              },
              "work": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "value": {
                      "type": "string",
                      "example": "email@gmail.com"
                    }
                  }
                }
              }
            }
          },
          "first_name": {
            "type": "string",
            "example": "John"
          },
          "skills": {
            "type": "array",
            "nullable": true,
            "items": {
              "type": "string"
            },
            "example": ["Finance", "Tech"]
          },
          "description": {
            "type": "string",
            "nullable": true,
            "example": "I am a finance manager at a bank"
          },
          "position": {
            "type": "string",
            "nullable": true,
            "example": "Chief Marketing Officer"
          },
          "year_joined": {
            "type": "integer",
            "nullable": true,
            "example": 2002
          },
          "secondary_position": {
            "type": "array",
            "nullable": true,
            "items": {
              "type": "object",
              "properties": {
                "company_id": {
                  "type": "string",
                  "example": "f000000120808"
                },
                "company_name": {
                  "type": "string",
                  "example": "Firmable"
                },
                "company_size": {
                  "type": "string",
                  "example": "1 - 100"
                },
                "company_website": {
                  "type": "string",
                  "example": "https://firmable.com"
                },
                "company_linkedin": {
                  "type": "string",
                  "example": "company-slug"
                },
                "company_industry": {
                  "type": "string",
                  "example": "Financial Services"
                },
                "cleaned_position": {
                  "type": "string",
                  "example": "Marketing Principal"
                },
                "month_joined": {
                  "type": "number",
                  "example": 11
                },
                "year_joined": {
                  "type": "number",
                  "example": "2022"
                },
                "time_in_current_role": {
                  "type": "string",
                  "example": "Nov '22 - Present"
                },
                "seniority": {
                  "type": "string",
                  "example": "Other"
                },
                "department": {
                  "type": "string",
                  "example": "Marketing"
                }
              }
            }
          }
        }
      },
      "Plant": {
        "type": "object",
        "properties": {
          "name": {
            "description": "The name of the plant",
            "type": "string"
          },
          "tag": {
            "description": "Tag to specify the type",
            "type": "string"
          }
        }
      },
      "NewPlant": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Plant"
          },
          {
            "required": ["id"],
            "type": "object",
            "properties": {
              "id": {
                "description": "Identification number of the plant",
                "type": "integer",
                "format": "int64"
              }
            }
          }
        ]
      },
      "Error": {
        "required": ["error", "message"],
        "type": "object",
        "properties": {
          "error": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}
